  console.log(id,time)\n            //console.log(this.ticket_obj)\n            const data = {\n                id:id,\n                time:time\n            }\n            //console.log(data['id'])\n            await axios.put('/api/ticket/',data)\n            .then(res => {\n                console.log(res)\n                const id = this.$store.state.user.id\n                axios.get(`api/single_ticket/${id}`)\n                .then(res => {\n                    this.ticket_obj = res.data['data']\n\n                })\n                \n            })\n            //console.log(data)\n        }\n    }\n}\n</script>\n<style >\n    \n</style>"],"sourceRoot":""}]}